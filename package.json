{
  "name": "v9s",
  "version": "1.0.5",
  "description": "TypeScript-based validations",
  "homepage": "https://vueent.github.io/v9s/",
  "repository": {
    "type": "git",
    "url": "git@github.com:vueent/v9s.git"
  },
  "bugs": {
    "url": "https://github.com/vueent/v9s/issues"
  },
  "main": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf ./lib && rollup --config rollup.config.mjs",
    "test": "npm run test:clear && npm run test:unit",
    "test:clear": "jest --clearCache",
    "test:unit": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "prepublishOnly": "npm run build && npm run test:unit",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "docs:dev": "vuepress dev docs-src/src",
    "docs:build": "vuepress build --dest ./docs docs-src/src"
  },
  "keywords": [
    "vueent",
    "typescript",
    "validations"
  ],
  "author": "VueenT team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.10.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.4.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.54.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "vuepress": "^1.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
